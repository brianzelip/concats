{
  "name": "concats",
  "description": "Output a single-column csv file containing rows of concatenated fields from an input delimiter-separated values file. Desktop app built using Electron.js and Vue.js.",
  "version": "1.13.10",
  "private": false,
  "author": {
    "name": "Brian Zelip"
  },
  "license": "MIT",
  "homepage": "https://github.com/brianzelip/concats",
  "repository": "https://github.com/brianzelip/concats",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "ajv": "^6.10.0",
    "csvtojson": "^2.0.8",
    "vue": "^2.5.21"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-service": "^3.8.0",
    "babel-eslint": "^10.0.1",
    "electron": "^2.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "vue-cli-plugin-electron-builder": "^1.3.4",
    "vue-svg-loader": "^0.11.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "main": "background.js"
}
